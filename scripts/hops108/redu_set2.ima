! Reduce HOPS-108 setting 2 data

sic parallel

@ ../common/redu init

let redu%basename hops108
let redu%setting 2
@ set_redu_params

! Spectral window 25 has a few channels with different weights. Ignore
! these differences so that we can use a single beam over the entire
! band.
let beam_gap 10

@ ../common/redu run
@ ../common/redu clean

! Reduce HOPS-108 setting 8 data

sic parallel

@ ../common/redu init

let redu%basename hops108
let redu%setting 8
@ set_redu_params

! Spectral window 31 has a few channels with a different weight at the
! center of the band. Ignore these differences so that we can use a
! single beam over the entire band.
let beam_gap 10

@ ../common/redu run
@ ../common/redu clean

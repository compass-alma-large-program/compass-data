! Reduce V883-Ori setting 6 data

sic parallel

@ ../common/redu init

let redu%basename v883ori
let redu%setting 6
@ set_redu_params

! Increase clipping value and decrease the number of spectral
! smoothing for line detection
let redu%uv_preview_clip 6
let redu%uv_preview_smooth 2

@ ../common/redu run
@ ../common/redu clean

! Reduce V883-Ori setting 1 data

sic parallel

@ ../common/redu init

let redu%basename v883ori
let redu%setting 1
@ set_redu_params

! Increase clipping value, decrease the number of spectral smoothing,
! and decrease the number of spatial scales for line detection.
let redu%uv_preview_clip 5
let redu%uv_preview_smooth 2
let redu%uv_preview_taper 2

@ ../common/redu run
@ ../common/redu clean

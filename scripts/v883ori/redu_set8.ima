! Reduce V883-Ori setting 8 data

sic parallel

@ ../lib/redu init

let redu%basename v883ori
let redu%setting 8
@ set_redu_params

! Spectral window 31 has a few channels with different weights. Ignore
! these differences so that we can use a single beam over the entire
! band.
let beam_gap 6

@ ../lib/redu run
@ ../lib/redu clean

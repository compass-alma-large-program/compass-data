! Reduce B335 setting 2 data

sic parallel

@ ../lib/redu init

let redu%basename b335
let redu%setting 2
@ set_redu_params

! Increase clipping value and decrease the number of spectral
! smoothing for line detection
let redu%uv_preview_clip 5
let redu%uv_preview_smooth 2

@ ../lib/redu run
@ ../lib/redu clean

! Reduce L1551-IRS5 setting 9 data

sic parallel

@ ../common/redu init

let redu%basename l1551irs5
let redu%setting 9
@ set_redu_params

! Increase clipping value and decrease the smoothing parameter for
! line detection
let redu%uv_preview_clip 5
let redu%uv_preview_smooth 2

! Use the same spatial grid for all spectral windows
let redu%map_size 2304 2304
let redu%map_cell 0.017 0.017

@ ../common/redu run
@ ../common/redu clean

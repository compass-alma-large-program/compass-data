! Reduce HOPS-373 setting 7 data

sic parallel

@ ../lib/redu init

let redu%basename hops373
let redu%setting 7
@ set_redu_params

! Increase clipping value for line detection
let redu%uv_preview_clip 7

! Spectral window 31 has a few channels with different weights. Ignore
! these differences so that we can use a single beam over the entire
! band.
let beam_gap 5

@ ../lib/redu run
@ ../lib/redu clean

! Reduce IRAS4A setting 7 data

sic parallel

@ ../lib/redu init

let redu%basename iras4a
let redu%setting 7
@ set_redu_params

! Increase clipping value and decrease the number of spectral
! smoothing for line detection
let redu%uv_preview_clip 6
let redu%uv_preview_smooth 1

@ ../lib/redu run
@ ../lib/redu clean
